if _G.HAPPY_script then
    warn("Script Ä‘Ã£ cháº¡y! KhÃ´ng thá»ƒ cháº¡y láº¡i.")
    return
end
_G.HAPPY_script = true

local function waitLoad(name, pct)
    if shared.HAPPY_LOADER_UPDATE then
        shared.HAPPY_LOADER_UPDATE(name, pct)
    end
end

game.StarterGui:SetCore("SendNotification", {
    Title = "âšœï¸HAPPY script v0.06âšœï¸";
    Text = "Good LuckğŸ€";
    Duration = 5;
})

---------------------------------------------------------------------
-- MOBILE CONFIRM LOADER
---------------------------------------------------------------------
local Players = game:GetService("Players")
local player = Players.LocalPlayer

local LoadingGui = player:WaitForChild("PlayerGui"):WaitForChild("HAPPYscriptLoadingScreen")
local Frame = LoadingGui:WaitForChild("Frame")
local UI = Frame:WaitForChild("MobileSupport")

local ConfirmButton = UI:WaitForChild("ConfirmButton")
local Icon = UI:WaitForChild("Icon")
local TitleLoad = UI:WaitForChild("TitleLoad")

-- Tráº¡ng thÃ¡i
local waitingForConfirm = false
local confirmCallback = nil

UI.Visible = false

-- Khi báº¥m nÃºt xÃ¡c nháº­n
ConfirmButton.MouseButton1Click:Connect(function()
    if waitingForConfirm and confirmCallback then
        UI.Visible = false
        waitingForConfirm = false
        confirmCallback()
    end
end)

-- HÃ m xÃ¡c nháº­n cho Mobile
local UserInputService = game:GetService("UserInputService")

local function MobileConfirmLoad(name, iconId)
    -- PC â†’ auto confirm (skip)
    if not UserInputService.TouchEnabled then
        return true
    end

    UI.Visible = true
    TitleLoad.Text = name
    Icon.Image = "rbxassetid://" .. tostring(iconId)

    waitingForConfirm = true

    -- Callback khi ngÆ°á»i dÃ¹ng báº¥m nÃºt
    local confirmed = false
    confirmCallback = function()
        confirmed = true
    end

    -- Chá» user báº¥m Confirm
    while not confirmed do
        task.wait()
    end

    UI.Visible = false
    return true
end

---------------------------------------------------------------------
-- HÃ€M LOAD URL CÃ“ Há»– TRá»¢ MOBILE
---------------------------------------------------------------------
local function SafeLoadStep(name, iconId, pct, url)
    waitLoad(name, pct)

    -- Báº®T BUá»˜C: Äá»£i Mobile confirm
    local mobileOk = MobileConfirmLoad(name, iconId)
    if not mobileOk then
        warn("Mobile confirm failed:", name)
        return
    end

    -- Load URL an toÃ n
    local ok, res = pcall(function()
        return loadstring(game:HttpGet(url))()
    end)

    if not ok then
        warn("FAILED TO LOAD:", name, res)
    end
end


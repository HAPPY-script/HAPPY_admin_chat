local function safeShow(button)
    if not button.Parent then
        repeat task.wait() until button.Parent
    end
    if button:IsA("GuiObject") then
        repeat task.wait() until button.AbsoluteSize.X > 0
    end
    for i = 1, 5 do
        button.Visible = true
        task.wait(0.1)
    end
end
safeShow(Button)

--------------------------------------------------
waitLoad("Checking dataüîç", 0.9)

loadstring(game:HttpGet("https://raw.githubusercontent.com/HAPPY-script/ADMIN/refs/heads/main/DataMember.lua"))()

waitLoad("‚úÖCOMPLETE‚úÖ", 1)
